{
  "name": "LTX-2 Text-to-Video (API)",
  "description": "LTX-2 distilled text-to-video workflow using Gemma encoder and two-stage sampling.",
  "version": "1.0.0",
  "workflow": {
    "1": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "ltx-2-19b-distilled-fp8.safetensors"
