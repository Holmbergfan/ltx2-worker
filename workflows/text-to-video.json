    },
    "30": {
      "class_type": "LTXVAudioVAEDecode",
      "inputs": {
        "samples": [
          "28",
          1
        ],
        "audio_vae": [
          "2",
          0
        ]
      }
    },
    "31": {
      "class_type": "CreateVideo",
      "inputs": {
        "images": [
          "29",
          0
        ],
        "fps": 24,
        "audio": [
          "30",
          0
        ]
      }
    },
    "32": {
      "class_type": "SaveVideo",
      "inputs": {
        "video": [
          "31",
          0
        ],
        "filename_prefix": "video/LTX-2",
        "format": "auto",
        "codec": "auto"
      }
    }
  }
}
