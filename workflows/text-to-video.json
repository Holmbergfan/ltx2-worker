    "29": {
      "class_type": "LTXVSpatioTemporalTiledVAEDecode",
      "inputs": {
        "vae": [
          "1",
          2
        ],
        "latents": [
          "28",
          0
        ],
        "spatial_tiles": 4,
        "spatial_overlap": 4,
        "temporal_tile_length": 16,
        "temporal_overlap": 4,
        "last_frame_fix": false,
        "working_device": "auto",
        "working_dtype": "auto"
